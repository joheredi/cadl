{
  "extends": "../../tsconfig.base.json",
  "references": [{ "path": "../compiler/tsconfig.json" }],
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": ".",
    "tsBuildInfoFile": "temp/tsconfig.tsbuildinfo",
    "jsx": "react-jsx",
    "jsxImportSource": "#jsx",
    "declarationMap": true,
    "declaration": true,
    "sourceMap": true,
  },
  "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "generated-defs/**/*.ts", "test/**/*.ts", "src/index.tsx"]
}
